{
    "name": "elixir/elixir-session",
    "description": "Component session",
    "version": "1.0.0",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "session"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-session",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-stdlib"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-dispatcher"
        }
    ],
    "require": {
        "php": ">=5.4",
        "elixir/elixir-stdlib": "*",
        "elixir/elixir-dispatcher": "*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\Session\\": "tests/"
        }
    },
    "suggest": {
    }
}